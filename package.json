{
  "name": "vortaik",
  "displayName": "vortaik",
  "description": "Powerful VS Code extension for building StreamElements widgets with HTML, CSS, JavaScript, and TypeScript. Includes obfuscation tools, Widget.io compatibility, Deno support, and multi-project management for streamlined, secure widget development.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vortaik.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "eslint": "^9.13.0",
    "postcss": "^8.4.47",
    "postcss-nesting": "^13.0.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "clean-css": "^5.3.3",
    "javascript-obfuscator": "^4.1.1",
    "jszip": "^3.10.1"
  }
}
